@page
@model RabbitRegister.Pages.Main.Rabbit.CreateRabbitModel
@{
}

<form method="post">
    <div class="form-group">
        <label asp-for="@Model.Rabbit.RabbitRegNo" class="control-label"></label>
        <input asp-for="@Model.Rabbit.RabbitRegNo" class="form-control" />
        <span asp-validation-for="@Model.Rabbit.RabbitRegNo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Rabbit.Name" class="control-label"></label>
        <input asp-for="@Model.Rabbit.Name" class="form-control" />
        <span asp-validation-for="@Model.Rabbit.Name" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="@Model.Rabbit.Rating" class="control-label"></label>
        <input asp-for="@Model.Rabbit.Rating" class="form-control" />
        <span asp-validation-for="@Model.Rabbit.Rating" class="text-danger"></span>
    </div>

    <p>
        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-primary" />
        </div>
    </p>
</form>